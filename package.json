{
  "name": "changelog-checker",
  "displayName": "Extension Update Reporter",
  "description": "Check changelogs of installed extensions and report their updates",
  "version": "0.1.0",
  "publisher": "ryu1kn",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/ryu1kn/vscode-changelog-checker/issues"
  },
  "homepage": "https://github.com/ryu1kn/vscode-changelog-checker/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryu1kn/vscode-changelog-checker.git"
  },
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "changelog"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Extension Update Reporter configurations",
      "properties": {
        "changelogChecker.extensionVersions": {
          "description": "Last checked version of installed extensions",
          "type": "object",
          "default": {}
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "coverage": "nyc yarn test",
    "lint": "tslint -p tslint.json",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && mocha --recursive out/test"
  },
  "dependencies": {
    "markdown-it": "8.4.1",
    "multiline-string": "0.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^7.0.43",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "testdouble": "^3.7.0",
    "ts-node": "^6.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.8.3",
    "vscode": "^1.1.16"
  }
}
